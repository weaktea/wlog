<WlogConfig>
    <ToStderr>false</ToStderr>
    <AlsoToStderr>false</AlsoToStderr>
    <!--0:DEBUG 1:INFO 2:WARNING 3:ERROR 4:FATAL-->
    <StderrThreshold>3</StderrThreshold>
    <TraceLocation>
        <File></File>
        <Line>0</Line>
    </TraceLocation>
    <!--0:DEBUG 1:INFO 2:WARNING 3:ERROR 4:FATAL-->
    <LogThreshold>1</LogThreshold>
    <!--ms-->
    <FlushInterval>0</FlushInterval>
    <LogDir>./log/</LogDir>
    <!--bytes-->
    <MaxSize>536870912</MaxSize>
</WlogConfig>